---
import HeaderLink from './HeaderLink.astro';
---
<header class="absolute inset-x-0 top-0 z-50">
        <nav aria-label="Global" class="flex items-center justify-between p-6 lg:px-8">
        <div class="flex lg:flex-1">
            <HeaderLink href="/" class ="text-onyx dark:text-pale-slate font-bold text-3xl font-wixdisplay rounded-md focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-cherry">
                Ava Daly</HeaderLink>
        </div>
        <div class="flex lg:hidden">
            <button type="button" command="show-modal" commandfor="mobile-menu" class="-m-2.5 inline-flex items-center justify-center rounded-md p-2.5 text-onyx dark:text-pale-slate">
            <span class="sr-only">Open main menu</span>
            <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5" data-slot="icon" aria-hidden="true" class="size-6">
                <path d="M3.75 6.75h16.5M3.75 12h16.5m-16.5 5.25h16.5" stroke-linecap="round" stroke-linejoin="round" />
            </svg>
            </button>
        </div>
        <div class="hidden lg:flex lg:gap-x-12">
			<HeaderLink href="/about" class="text-lg rounded-md focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-cherry">About</HeaderLink>
			<HeaderLink href="/projects" class="text-lg rounded-md focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-cherry">Projects</HeaderLink>
			<HeaderLink href="/contact" class="text-lg rounded-md focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-cherry">Contact</HeaderLink>
